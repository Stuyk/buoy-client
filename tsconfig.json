{
    "compilerOptions": {
        "downlevelIteration": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "importHelpers": true,
        "isolatedModules": true,
        "lib": ["ES2020", "DOM"],
        "module": "ES2020",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "sourceMap": true,
        "strict": true,
        "target": "ES2020"
    },
    "include": ["src/**/*"]
}
